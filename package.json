{
  "name": "@angiejones/mcp-selenium",
  "version": "0.1.21",
  "description": "Selenium WebDriver MCP Server",
  "type": "module",
  "main": "src/lib/server.js",
  "bin": {
    "mcp-selenium": "./src/lib/server.js"
  },
  "scripts": {
    "start": "node src/lib/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "jiomart": "node jiomart-location-selector.js",
    "jiomart:location": "node jiomart-location-selector.js",
    "api": "node api-server.js",
    "api:server": "node api-server.js",
    "scraper-api": "node scraper-api-server.js",
    "scraper": "node scraper-api-server.js",
    "scraper-ui": "node scraper-api.js",
    "ui": "node scraper-api.js",
    "frontend": "node frontend-server.js",
    "backend": "node scraper-api-server.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "quickapi": "node quickapi.js",
    "quick": "node quickapi.js",
    "docker:build": "docker build -t quickapi-scraper .",
    "docker:run": "docker run -d --name quickapi -p 3001:3001 --shm-size=2gb quickapi-scraper",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker stop quickapi || docker-compose stop",
    "docker:clean": "docker-compose down -v && docker rmi quickapi-scraper || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "cheerio": "^1.1.2",
    "crawl4ai": "^1.0.1",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "playwright": "^1.57.0",
    "puppeteer": "^24.34.0",
    "selenium-webdriver": "^4.39.0",
    "tesseract.js": "^7.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
